                                    <div class="c-hot-hotel-item-v2__image-clipping">
                                        <a href="<% if (typeof user !== 'undefined' && user) { %>/hotel-details/<%= hotel.slug %><% } else { %>#<% } %>"
                                           <% if (!(typeof user !== 'undefined' && user)) { %>onclick="alert('Login is required');"<% } %>>
                                            <div class="c-hot-hotel-item-v2__image aimg">
                                                <img src="/uploads/<%= hotel.banner_image_id || '/images/default-hotel.jpg' %>" alt="<%= hotel.title %>">
                                                <div class="c-hot-hotel-item-v2__name">
                                                    <div class="hotel-title"><%= hotel.title %></div>
                                                    <div class="view-hotel">VIEW HOTEL ></div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </article>
                        </div>
                    <% }) %>
                </div>
            <% } else { %>
                <p>No hotels found matching your search.</p>
            <% } %>
        </div>

        <!-- 골프 탭 -->
        <div class="tab-pane fade"  role="tabpanel">
            <% if (golfCourses && golfCourses.length > 0) { %>
                <div class="row">
                    <% golfCourses.forEach((golf, index) => { %>
                        <div class="col-md-6 wow-outer">
                            <article class="post-modern wow slideInLeft">
                                <div class="c-hot-hotel-item-v2__image-con mgb30">
                                    <div class="c-hot-hotel-item-v2__image-clipping">
                                        <a href="<% if (typeof user !== 'undefined' && user) { %>/golf-details/<%= golf.slug %><% } else { %>#<% } %>"
                                           <% if (!(typeof user !== 'undefined' && user)) { %>onclick="alert('Login is required');"<% } %>>
                                            <div class="c-hot-hotel-item-v2__image aimg">
                                                <img src="/uploads/<%= golf.banner_image_id || '/images/default-golf.jpg' %>" alt="<%= golf.title %>">
                                                <div class="c-hot-hotel-item-v2__name">
                                                    <div class="hotel-title"><%= golf.title %></div>
                                                    <div class="view-hotel">VIEW GOLF COURSE ></div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </article>
                        </div>
                    <% }) %>
                </div>
            <% } else { %>
                <p>No golf courses found matching your search.</p>
            <% } %>
        </div>

        <!-- 패키지 탭 -->
        <div class="tab-pane fade" id="packages" role="tabpanel">
            <% if (packages && packages.length > 0) { %>
                <div class="row">
                    <% packages.forEach((package, index) => { %>
                        <div class="col-md-6 wow-outer">
                            <article class="post-modern wow slideInLeft">
                                <div class="c-hot-hotel-item-v2__image-con mgb30">
                                    <div class="c-hot-hotel-item-v2__image-clipping">
                                        <a href="<% if (typeof user !== 'undefined' && user) { %>/package-details/<%= package.slug %><% } else { %>#<% } %>"
                                           <% if (!(typeof user !== 'undefined' && user)) { %>onclick="alert('Login is required');"<% } %>>
                                            <div class="c-hot-hotel-item-v2__image aimg">
                                                <img src="/uploads/<%= package.banner_image_id || '/images/default-package.jpg' %>" alt="<%= package.title %>">
                                                <div class="c-hot-hotel-item-v2__name">
                                                    <div class="hotel-title"><%= package.title %></div>
                                                    <div class="view-hotel">VIEW PACKAGE ></div>
                                                </div>
                                            </div>
                                        </a>
                                    </div>
                                </div>
                            </article>
                        </div>
                    <% }) %>
                </div>
            <% } else { %>
                <p>No packages found matching your search.</p>
            <% } %>
        </div>
    </div>

    <!-- 페이지네이션 -->
    <nav aria-label="Page navigation" class="mt-4">
        <ul class="pagination justify-content-center">
            <li class="page-item <%= currentPage === 1 ? 'disabled' : '' %>">
                <a class="page-link" href="?s=<%= encodeURIComponent(query) %>&page=1">&laquo;</a>
            </li>
            
            <% for(let i = 1; i <= totalPages; i++) { %>
                <li class="page-item <%= currentPage === i ? 'active' : '' %>">
                    <a class="page-link" href="?s=<%= encodeURIComponent(query) %>&page=<%= i %>"><%= i %></a>
                </li>
            <% } %>
            
            <li class="page-item <%= currentPage === totalPages ? 'disabled' : '' %>">
                <a class="page-link" href="?s=<%= encodeURIComponent(query) %>&page=<%= totalPages %>">&raquo;</a>
            </li>
        </ul>
    </nav>
</div>

<style>
    .c-hot-hotel-item-v2__image-con { margin-bottom: 30px; }
    .c-hot-hotel-item-v2__image-clipping {
        width: 100%;
        height: 327px;
        margin: 0 auto;
        position: relative;
        overflow: hidden;
        border-radius: 10px;
    }
    .c-hot-hotel-item-v2__image {
        width: 100%;
        height: 100%;
        position: relative;
    }
    .c-hot-hotel-item-v2__image img {
        width: 100%;
        height: 100%;
        object-fit: cover;
        transition: all 0.2s linear;
    }
    .c-hot-hotel-item-v2__name {
        position: absolute;
        bottom: 0;
        left: 0;
        right: 0;
        background: linear-gradient(to bottom, rgba(0,0,0,0), rgba(0,0,0,0.9));
        color: white;
        padding: 20px;
        text-align: left;
    }
    .hotel-title {
        font-size: 18px;
        font-weight: bold;
        margin-bottom: 5px;
    }
    .view-hotel {
        font-size: 14px;
        color: #007daa;
    }
    .aimg:hover img {
        transform: scale(1.1);
    }
    .nav-tabs .nav-link {
        color: #495057;
        border: 1px solid transparent;
        border-top-left-radius: .25rem;
        border-top-right-radius: .25rem;
        padding: .5rem 1rem;
        margin-right: 5px;
    }
    .nav-tabs .nav-link.active {
        color: #007bff;
        background-color: #fff;
        border-color: #dee2e6 #dee2e6 #fff;
    }
</style>

<%- include('../partials/footer') %>
<script src="/js/filterSearch.js"></script>
</body>
</html>
