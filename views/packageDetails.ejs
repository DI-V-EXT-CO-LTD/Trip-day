        <script>
            const today = new Date();

            function formatDate(date) {
                const year = date.getFullYear();
                const month = String(date.getMonth() + 1).padStart(2, '0');
                const day = String(date.getDate()).padStart(2, '0');
                return `${year}-${month}-${day}`;
            }

            function setMinDate() {
                const checkInDate = new Date(document.getElementById('checkIn').value);

                // ถ้าวันที่เช็คอินถูกเลือกแล้ว ให้ตั้ง min ของ checkOut เป็นวันถัดไปจาก checkIn
                if (checkInDate) {
                    const checkOutDate = new Date(checkInDate);
                    checkOutDate.setDate(checkInDate.getDate() + 1); // เพิ่ม 1 วันให้กับ checkIn

                    const checkOutInput = document.getElementById('checkOut');

                    // ตั้งค่า min ของ checkOut
                    checkOutInput.min = formatDate(checkOutDate);

                    // หาก checkOut น้อยกว่า checkIn (เลือกวันที่ก่อนหน้า) ให้ตั้งค่า value ของ checkOut เป็นวันถัดไปจาก checkIn
                    if (new Date(checkOutInput.value) < checkOutDate) {
                        checkOutInput.value = formatDate(checkOutDate);
                    }
                    updateNights()
                }
            }

            // ฟังก์ชันเพื่อกำหนดค่าเริ่มต้น
            function setDefaultDates() {
                // แปลงวันที่ให้อยู่ในรูปแบบ YYYY-MM-DD
                document.getElementById('checkIn').value = formatDate(today);
                document.getElementById('checkIn').min = formatDate(today);
                document.getElementById('checkOut').value = formatDate(new Date(today.getTime() + 24 * 60 * 60 * 1000)); // วันที่ถัดไป
                updateNights();
            }

            function updateNights() {
                const checkIn = new Date(document.getElementById('checkIn').value);
                const checkOut = new Date(document.getElementById('checkOut').value);

                if (checkOut > checkIn) {
                    const timeDifference = checkOut.getTime() - checkIn.getTime(); // คำนวณระยะเวลาเป็นมิลลิวินาที
                    const nights = Math.ceil(timeDifference / (1000 * 60 * 60 * 24)); // แปลงจากมิลลิวินาทีเป็นวัน
                    document.getElementById('nightsLabel').textContent = `${nights} night${nights > 1 ? 's' : ''}`;
                } else {
                    document.getElementById('nightsLabel').textContent = 'Invalid dates';
                }
            }

            document.getElementById('golf_input').addEventListener('input', function () {
                const adults = parseInt(document.getElementById('adults_input').value);
                let golf = parseInt(this.value);

                if (golf > adults) {
                    golf = adults; // ถ้าจำนวน golf มากกว่า adults ให้ปรับให้เท่ากับจำนวน adults
                    this.value = golf; // อัพเดตค่าใน input ของ golf
                }
            });

            document.getElementById('adults_input').addEventListener('input', function () {
                const adults = parseInt(this.value);
                let golf = parseInt(document.getElementById('golf_input').value);

                if (golf > adults) {
                    golf = adults; // ถ้าจำนวน golf มากกว่า adults ให้ปรับให้เท่ากับจำนวน adults
                    document.getElementById('golf_input').value = golf; // อัพเดตค่าใน input ของ golf
                }
            });



            const reserveButtons = document.getElementById('reserve-btn');
            reserveButtons.addEventListener('click', () => {
                const packageId = document.getElementById('packageId').value;
                const checkIn = document.getElementById('checkIn').value;
                const checkOut = document.getElementById('checkOut').value;
                const adults = document.getElementById('adults_input').value;
                const golf = document.getElementById('golf_input').value;

                // คำนวณจำนวนคืน (nights)
                const checkInDate = new Date(checkIn);
                const checkOutDate = new Date(checkOut);
                const nights = Math.ceil((checkOutDate - checkInDate) / (1000 * 60 * 60 * 24));



                addToCart(checkIn, checkOut, adults, nights, golf, packageId);
            });





            function addToCart(checkIn, checkOut, adults, nights, golf, packageId) {
                fetch('/cart/add', {
                    method: 'POST',
                    headers: {
                        'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({
                        packageId,
                        checkIn,
                        checkOut,
                        adults,
                        nights,
                        golf
                    }),
                })
                    .then(response => {
                        if (!response.ok) {
                            throw new Error(`HTTP error! status: ${response.status}`);
                        }
                        return response.json();
                    })
                    .then(data => {
                        if (data.success) {
                            if (confirm('Room added to cart successfully! Do you want to view your cart?')) {
                                window.location.href = '/dashboard#cart';
                            }
                            // You can update the UI here to reflect the change, e.g., update a cart icon
                        } else {
                            alert(data.message || 'Failed to add room to cart. Please try again.');
                        }
                    })
                    .catch((error) => {
                        console.error('Error:', error);
                        alert('An error occurred while adding the room to the cart. Please check the console for more details and try again.');
                    });
            }


            // เรียกใช้งานฟังก์ชันเพื่อกำหนดค่าเริ่มต้นเมื่อโหลดหน้าเว็บ
            setDefaultDates();

            // ตั้งค่า min ของ checkIn และ checkOut เมื่อเลือกวันที่
            document.getElementById('checkIn').addEventListener('change', setMinDate);
        </script>